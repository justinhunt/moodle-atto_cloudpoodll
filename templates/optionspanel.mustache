                  <option value="{{LANG.ENUS}}" {{#LANG.useENUS}}selected="selected"{{/LANG.useENUS}}>{{#str}} en-us, atto_cloudpoodll {{/str}}</option>
                  <option value="{{LANG.ENGB}}" {{#LANG.useENGB}}selected="selected"{{/LANG.useENGB}}>{{#str}} en-gb, atto_cloudpoodll {{/str}}</option>
                  <option value="{{LANG.ENAU}}" {{#LANG.useENAU}}selected="selected"{{/LANG.useENAU}}>{{#str}} en-au, atto_cloudpoodll {{/str}}</option>
                  <option value="{{LANG.ENIN}}" {{#LANG.useENIN}}selected="selected"{{/LANG.useENIN}}>{{#str}} en-in, atto_cloudpoodll {{/str}}</option>
                  <option value="{{LANG.ENIE}}" {{#LANG.useENIE}}selected="selected"{{/LANG.useENIE}}>{{#str}} en-ie, atto_cloudpoodll {{/str}}</option>
