                  <option value="{{LANG.ESES}}" {{#LANG.useESES}}selected="selected"{{/LANG.useESES}}>{{#str}} es-es, atto_cloudpoodll {{/str}}</option>
                  <option value="{{LANG.FAIR}}" {{#LANG.useFAIR}}selected="selected"{{/LANG.useFAIR}}>{{#str}} fa-ir, atto_cloudpoodll {{/str}}</option>
