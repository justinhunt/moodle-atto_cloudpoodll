  {{!
    @template atto_cloudpoodll/options

    CloudPoodll options template.

    The purpose of this template is to render recorder type history by user.
  }}
  <div data-medium-type="{{CSS.OPTIONS}}" data-field="options" class="tab-pane" id="{{elementid}}_{{CSS.OPTIONS}}">
      <br><label>
          <input type="checkbox" id="{{elementid}}_{{CSS.MEDIAINSERT_CHECKBOX}}" class="{{CSS.MEDIAINSERT_CHECKBOX}}"
          {{#mediataginsert}} checked="true"{{/mediataginsert}}
          />&nbsp;{{#str}} mediainsertcheckbox, atto_cloudpoodll {{/str}}
          </label>
      {{#cansubtitle}}
          <br><label>
              <input type="checkbox" id="{{elementid}}_{{CSS.SUBTITLE_CHECKBOX}}" class="{{CSS.SUBTITLE_CHECKBOX}}"
              {{#isvideo}}{{#letssubtitlevideo}} checked="true"{{/letssubtitlevideo}}{{/isvideo}}
              {{^isvideo}}{{#letssubtitleaudio}} checked="true"{{/letssubtitleaudio}}{{/isvideo}}
              />
              &nbsp;{{#str}} subtitlecheckbox, atto_cloudpoodll {{/str}}
              </label>
          <br><label>{{#str}} speakerlanguage, atto_cloudpoodll{{/str}}&nbsp;
              <select id="{{elementid}}_{{CSS.LANG_SELECT}}" class="{{CSS.LANG_SELECT}}">
                  <option value="{{LANG.ARAE}}" {{#LANG.useARAE}}selected="selected"{{/LANG.useARAE}}>{{#str}} ar-ae, atto_cloudpoodll {{/str}}</option>
